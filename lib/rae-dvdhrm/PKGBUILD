pkgname="rae-dvdhrm"
pkgdesc="Personal Package of dvdhrm"
pkgver=1.0.0
pkgrel=1

arch=("any")
license=("Apache")
url="https://github.com/readaheadeu/rae-aur"

source=(
        "sway.service"
)

sha256sums=(
        "SKIP"
)

package() {
        mkdir -p "$pkgdir/usr/lib/systemd/user"
        cp "$srcdir/sway.service" "$pkgdir/usr/lib/systemd/user/"
}
